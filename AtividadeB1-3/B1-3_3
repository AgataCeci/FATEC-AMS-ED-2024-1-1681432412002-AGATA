#include <stdio.h>

float notas[5];
float value;

void imprimeArray(float notas[]){
    for (int i=0; i<5; i++){
        printf("Nota %d: %.2f\n", i+1, notas[i]);
  }
}

int main(void) {

  for (int i = 0; i < 5; i++) {
    printf("\nDigite as notas: \n");
    scanf("%f", &value);
    notas[i] = value;
  }

  printf("\n");
  
  printf("As notas sÃ£o: \n");

  printf("\n");
  
  imprimeArray(notas);
  
  return 0;

}
