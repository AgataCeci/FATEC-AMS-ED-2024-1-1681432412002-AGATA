#include <stdio.h>

float notas[5];
float value;
int valQual;

void notasMulti(float notas[], int *vaQual){
  
    for (int i=0; i<5; i++){
      printf("%.2f\n", notas[i]**vaQual);
  }

}

int main(void) {

  for (int i = 0; i < 5; i++) {
    printf("\nDigite as notas: \n");
    scanf("%f", &value);
    notas[i] = value;
  }

  printf("\nDigite um valor qualquer: \n");
  scanf("%d", &valQual);

  printf("\n");

  printf("As notas vezes %d sÃ£o: \n", valQual);
  
  notasMulti(notas, &valQual);
  
  return 0;

}
